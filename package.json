{
  "name": "gatsby-site-demo-server",
  "version": "1.0.0",
  "description": "Server for gatsby site",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon server.js",
    "dev:all": "concurrently \"nodemon server.js --ignore public/\" \"webpack --watch\"",
    "startup": "node server.js",
    "watch-client-side": "browser-sync start --proxy=localhost:3000/api/dashboard --files='./public/**/*.css, ./public/**/*.js, **/*.pug' --ignore=node_modules --reload-delay 10 --no-ui --no-open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwalker2276/gatsby-site-demo-server.git"
  },
  "author": "Jordan Walker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwalker2276/gatsby-site-demo-server/issues"
  },
  "homepage": "https://github.com/jwalker2276/gatsby-site-demo-server#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-security": "^1.4.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jimp": "^0.8.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.4",
    "sequelize": "^5.15.1",
    "validator": "^11.1.0"
  }
}
