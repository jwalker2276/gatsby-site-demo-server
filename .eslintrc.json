{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended",
    "plugin:security/recommended"
  ],
  "plugins": ["prettier", "security"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "camelcase": 0,
    "consistent-return": 0,
    "no-param-reassign": 1,
    "node/no-unpublished-require": 0,
    "node/no-unsupported-features/es-syntax": 0
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
