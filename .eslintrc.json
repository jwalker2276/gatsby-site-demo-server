{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended",
    "plugin:security/recommended"
  ],
  "plugins": ["prettier", "security"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "camelcase": 0,
    "consistent-return": 0
  }
}
